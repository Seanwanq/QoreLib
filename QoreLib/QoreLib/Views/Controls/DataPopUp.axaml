<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:cvm="clr-namespace:QoreLib.ViewModels.ControlsViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="QoreLib.Views.Controls.DataPopUp"
             x:DataType="cvm:DataPopUpViewModel">
    <Border CornerRadius="8" Background="White" BorderThickness="1">
        <Border.BorderBrush>
            <SolidColorBrush Color="Black" Opacity="0.2"></SolidColorBrush>
        </Border.BorderBrush>
        <Grid>
            <TextBlock>Data Pop Up</TextBlock>
        </Grid>
    </Border>
</UserControl>
